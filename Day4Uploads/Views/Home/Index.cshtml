@model IEnumerable<Day4Uploads.Models.ImageUpload>

@foreach (var upload in Model)
{
    <div class="row">
        <div class="col-md-3">
            <img src="@upload.FilePath" alt="@upload.Caption" />
        </div>
        <div class="col-md-9">
            <h2>@upload.Caption</h2>
        </div>
    </div>
}